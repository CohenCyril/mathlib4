  exact hŒ≥.comp_add _

lemma isIntegralCurve_comp_add {Œ≥ : ‚Ñù ‚Üí M} {dt : ‚Ñù} : IsIntegralCurve Œ≥ v ‚Üî
    IsIntegralCurve (Œ≥ ‚àò (¬∑ + dt)) v := by
  refine ‚ü®fun hŒ≥ => hŒ≥.comp_add _, fun hŒ≥ ‚Ü¶ ?_‚ü©
  convert hŒ≥.comp_add (-dt)
